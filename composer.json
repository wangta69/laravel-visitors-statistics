{
  "name": "wangta69/laravel-visitors-statistics",
  "description": "Visitor tracker and statistics package for Laravel.",
  "type": "library",
  "homepage": "https://www.onstory.fun/packages/laravel-visitors-statistics",
  "license": "MIT",
  "authors": [
    {
      "name": "Pondol",
      "email": "wangta69@gmail.com",
      "homepage": "https://www.onstory.fun"
    }
],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.0.0",
    "geoip2/geoip2": "~2.0",
    "wangta69/laravel-chart": "*",
    "wangta69/laravel-pondol": "*",
    "matomo/device-detector": "^6.4"
  },
  "require-dev": {
    "orchestra/testbench": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "Pondol\\VisitorsStatistics\\": "src/"
    }
  },
  "autoload-dev": {
      "psr-4": {
        "Pondol\\VisitorsStatistics\\Tests\\": "tests/"
      }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pondol\\VisitorsStatistics\\VisitorStatisticsServiceProvider"
      ]
    }
  }
}
